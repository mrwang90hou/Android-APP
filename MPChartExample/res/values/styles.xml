<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.










    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <!-- Customize your theme here. -->
       <!-- <item name="colorPrimary">@color/text_orange</item>
        <item name="colorPrimaryDark">@color/text_orange</item>
        <item name="colorAccent">@color/text_orange</item>-->
        <!-- Customize your theme here. -->
        <!-- Action Bar Theme -->
        <!-- Custom Style Attribute-->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- Dialog样式  （阴影） -->
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
    </style>

    <style name="http_dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <!-- 我的页面的网格面板样式 -->
    <!--
        android:layout_width="0dp" android:layout_weight="1" android:layout_height="wrap_content"
                  android:orientation="vertical" android:gravity="center"









    -->
    <!-- 标签文字样式 -->
    <style name="text_tag">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_gray_general</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:layout_margin">4dp</item>
    </style>
    <!-- 普通灰色文字样式 -->
    <style name="text_gray_general">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_gray_general</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 普通橘色文字样式 -->
    <style name="text_orange_general">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_orange</item>
        <item name="android:drawablePadding">2dp</item>
       <!--  <item name="android:textSize">18sp</item> -->
    </style>
    <!-- 企业介绍文字样式 -->
    <style name="enterprise_switch_text">
